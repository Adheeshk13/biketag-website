<script nonce="{{nonce}}">
	window.dataLayer = window.dataLayer || [];

	function gtag() {
		dataLayer.push(arguments);
	}
	gtag('js', new Date());
	gtag('config', '{{ page.meta.gaUA }}');
</script>
