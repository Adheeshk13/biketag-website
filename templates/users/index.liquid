<!DOCTYPE html>
<html lang="en">
	<head>
		{% include "page/meta" %}
		{% include "page/css" %}
		
		<!-- Bootstrap -->
		{{ "/css/bootstrap.min.css" | stylesheet_tag }}
		{{ "/css/fa-all.min.css" | stylesheet_tag }}
		{{ images | json | console_log: nonce }}
	</head>

	<body>
		<h1>Pick a user from the list below!</h1>

		<section>
			<ul class="list-group">
			{% for username in usernames %}
				<li class="list-group-item">
					<a href="/user/?username={{ username }}">{{ username }}</a>
				</li>
			{% endfor %}
			</ul>
		</section>

		{{ "/js/jquery.min.js" | javascript_tag }}
		<!-- Bootstrap -->
		{{ "/js/bootstrap.min.js" | javascript_tag }}
	</body>
</html>
